{
    "version": 2,
    "builds": [
        {
        "src": "./api/index.js",
        "use": "@vercel/node"
        },
        {
        "src": "./app/build",   
        "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
        "source": "/(.*)",
        "destination": "/backend/server.js"
        }
    ]
}